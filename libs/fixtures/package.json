    "is-ci-cli": "2.2.0",
    "jest": "^29.6.2",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "11.0.0",
    "sort-package-json": "^1.50.0",
