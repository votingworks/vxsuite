{
  "name": "@votingworks/res-to-ts",
  "version": "1.0.0",
  "private": true,
  "description": "Converts a resource file to a TypeScript file",
  "keywords": [],
  "license": "GPL-3.0-only",
  "type": "module",
  "main": "src/index.ts",
  "bin": {
    "res-to-ts": "bin/res-to-ts"
  },
  "scripts": {
    "type-check": "tsc --build",
    "build": "pnpm --filter $npm_package_name... build:self",
    "build:self": "pnpm type-check",
    "clean": "pnpm --filter $npm_package_name... clean:self",
    "clean:self": "rm -rf build && tsc --build --clean",
    "lint": "pnpm type-check && eslint .",
    "lint:fix": "pnpm type-check && eslint . --fix",
    "test": "is-ci test:ci test:watch",
    "test:ci": "vitest --coverage --run --reporter junit --outputFile reports/junit.xml",
    "test:coverage": "vitest --coverage --run",
    "test:watch": "vitest"
  },
  "packageManager": "pnpm@8.15.5",
  "dependencies": {
    "globby": "11",
    "js-sha256": "^0.9.0",
    "mime": "^3.0.0",
    "tmp": "^0.2.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/mime": "^2.0.3",
    "@types/node": "20.16.0",
    "@types/tmp": "0.2.4",
    "@vitest/coverage-istanbul": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "@votingworks/test-utils": "workspace:*",
    "esbuild": "0.21.2",
    "esbuild-runner": "2.2.2",
    "eslint-plugin-vx": "workspace:*",
    "is-ci-cli": "2.2.0",
    "vitest": "^2.1.8"
  }
}
