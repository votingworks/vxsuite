{
  "name": "@votingworks/ballot-interpreter-vx",
  "version": "5.2.16",
  "description": "Interprets hand-marked paper ballots.",
  "repository": "https://github.com/votingworks/ballot-interpreter-vx",
  "license": "GPL-3.0",
  "author": "VotingWorks Eng <eng@voting.works>",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "bin": {
    "ballot-interpreter-vx": "bin/ballot-interpreter-vx.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "type-check": "tsc --build",
    "type-check:watch": "tsc --build --watch",
    "build": "tsc --build tsconfig.build.json",
    "build:watch": "tsc --build --watch tsconfig.build.json",
    "clean": "rm -rf build tsconfig.tsbuildinfo tsconfig.build.tsbuildinfo",
    "lint": "pnpm type-check && eslint .",
    "lint:fix": "pnpm type-check && eslint . --fix",
    "prepack": "tsc --build",
    "test": "is-ci test:ci test:watch",
    "test:ci": "pnpm test:coverage",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.+(css|graphql|json|less|md|mdx|sass|scss|yaml|yml)": [
      "prettier --write"
    ],
    "*.+(js|jsx|ts|tsx)": [
      "eslint --quiet --fix"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "dependencies": {
    "@votingworks/ballot-encoder": "workspace:*",
    "@votingworks/image-utils": "workspace:*",
    "@votingworks/qrdetect": "^1.0.1",
    "@votingworks/types": "workspace:*",
    "@votingworks/utils": "workspace:*",
    "buffer": "^6.0.3",
    "canvas": "2.9.1",
    "chalk": "^4.1.0",
    "debug": "^4.2.0",
    "jsfeat": "^0.0.8",
    "jsqr": "^1.3.1",
    "node-quirc": "^2.2.1",
    "table": "^6.0.3",
    "util": "^0.12.4",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/benchmark": "^1.0.33",
    "@types/debug": "^4.1.5",
    "@types/jest": "^27.0.3",
    "@types/jsfeat": "workspace:*",
    "@types/memorystream": "^0.3.0",
    "@types/node": "16.11.29",
    "@types/node-quirc": "workspace:*",
    "@types/table": "^6.0.0",
    "@types/tmp": "^0.2.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "@votingworks/fixtures": "workspace:*",
    "benchmark": "^2.1.4",
    "esbuild": "^0.14.25",
    "esbuild-runner": "^2.2.1",
    "eslint": "^8.23.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-vx": "workspace:*",
    "is-ci-cli": "^2.2.0",
    "jest": "^27.3.1",
    "jest-watch-typeahead": "^0.6.4",
    "lint-staged": "^10.4.2",
    "memorystream": "^0.3.1",
    "prettier": "^2.6.2",
    "sort-package-json": "^1.50.0",
    "tmp": "^0.2.1",
    "ts-jest": "^27.0.7",
    "typescript": "4.6.3"
  },
  "packageManager": "pnpm@5.18.10"
}
