{
  "name": "eslint-plugin-vx",
  "version": "1.0.0",
  "description": "VotingWorks monorepo rules for ESLint",
  "keywords": [],
  "license": "GPL-3.0",
  "author": "VotingWorks Eng <eng@voting.works>",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "type-check": "tsc --build",
    "type-check:watch": "tsc --build --watch",
    "build": "tsc --build tsconfig.build.json",
    "build:watch": "tsc --build --watch tsconfig.build.json",
    "clean": "rm -rf build tsconfig.tsbuildinfo tsconfig.build.tsbuildinfo",
    "lint": "eslint . --ext ts",
    "lint:fix": "eslint . --ext ts --fix",
    "test": "is-ci test:coverage test:watch",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "packageManager": "pnpm@5.18.10",
  "dependencies": {
    "@types/eslint": "^8.4.1",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/experimental-utils": "^5.21.0",
    "comment-parser": "^1.2.4",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "typescript": "4.6.3"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.29",
    "@types/react": "^17.0.20",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vx": "workspace:*",
    "is-ci-cli": "^2.2.0",
    "jest": "^27.3.1",
    "jest-environment-node": "^27.0.3",
    "jest-watch-typeahead": "^0.6.4",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "ts-jest": "^27.0.7"
  }
}