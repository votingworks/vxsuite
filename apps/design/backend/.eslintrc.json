{
  "extends": ["plugin:vx/recommended"],
  "rules": {
    // Disable JSDOC rule as code is self-documenting.
    "vx/gts-jsdoc": "off"
  },
  "overrides": [
    {
      "files": ["scripts/**"],
      "rules": {
        "no-console": "off"
      }
    },
    {
      "files": ["migrations/**"],
      "rules": {
        "vx/gts-module-snake-case": "off"
      }
    }
  ]
}
